{
  "jobs": [
    {
      "sObjectApiName": "Account",
      "dataFileName": "Account.json",
      "deployConfig": {
        "externalIdFieldApiName": "AccountId__c",
        "maxWaitMinutes": 2
      },
      "retrieveConfig": {
        "fieldApiNames": ["AccountId__c", "Name"],
        "filterCriteria": {
          "Name": { "$like": "Demo %" }
        },
        "sortFieldNames": ["Name"]
      }
    },
    {
      "sObjectApiName": "Contact",
      "dataFileName": "Contact.json",
      "deployConfig": {
        "externalIdFieldApiName": "ContactId__c",
        "maxWaitMinutes": 3
      },
      "retrieveConfig": {
        "fieldApiNames": ["ContactId__c", "FirstName", "LastName", "Email", "Account.AccountId__c"],
        "filterCriteria": {
          "LastName": "Wittberger"
        },
        "sortFieldNames": ["LastName"]
      }
    }
  ]
}
